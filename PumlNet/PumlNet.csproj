<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>PumlNet</Title>
        <Authors>KifoPL</Authors>
        <Description>Parse your .NET codebase into PlantUML</Description>
        <RepositoryUrl>https://github.com/KifoPL/PumlNet</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageIcon>PumlNet_logo.png</PackageIcon>
    </PropertyGroup>

    <ItemGroup>
        <None Include="PumlNet_logo.png" Pack="true" PackagePath="" />
    </ItemGroup>

</Project>
